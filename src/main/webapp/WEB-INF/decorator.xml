<?xml version="1.0" encoding="EUC-KR"?>
<!-- /WebContent/WEB-INF/decorators.xml -->
<decorators defaultdir="/decorator">
	
	<decorator page="decorator.jsp" name="main">
		<pattern>*.sms</pattern>
	</decorator>
	<decorator page="decorator2.jsp" name="buildingDetail">
		<pattern>/building/buildingDetail.sms*</pattern>
		<pattern>/reserve/regReserve.sms*</pattern>
		<pattern>/becomeahost_complete.sms*</pattern>
		<pattern>/lets*</pattern>
	</decorator>
	<decorator page="decorator3.jsp" name="buildingDetail2">
		<pattern>/login.sms*</pattern>
		<pattern>/personal*</pattern>
		<pattern>/after_delete*</pattern>
		<pattern>/joinForm.sms*</pattern>
		<pattern>/find*</pattern>
	</decorator>
	
	
	<!-- layout 설정을 제외 함. -->
	<excludes>
		<pattern>/admin/graph*</pattern>
		<pattern>/user/mygraph*</pattern>
		<pattern>*find_address*</pattern>
		<pattern>/trans/graph*</pattern>
		<pattern>/trans/graph*</pattern>
		<pattern>/qa/questionList.sms*</pattern>
		<pattern>/building/Qlist.sms*</pattern>
		<pattern>/building/Rlist.sms*</pattern>
		<pattern>/building/Rreply.sms*</pattern>
		<pattern>/building/Qreply.sms*</pattern>
		<pattern>/building/QlistNotRead.sms*</pattern>
		<pattern>/reserve/reserveCheckList.sms*</pattern>
		<pattern>/admin/adminAnswerQuestion.sms*</pattern>
		<pattern>/building/rImgList.sms*</pattern>
	</excludes>
</decorators>